{
  "name": "cca-delegate",
  "version": "0.1.2",
  "description": "Chrome Mobile Apps command delegate for using in programmatically on gulp/Grunt plug-ins",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com"
  },
  "repository": "ragingwind/cca-delegate",
  "license": "MIT",
  "files": [
    "index.js",
    "gadget.js"
  ],
  "keywords": [
    "cca-delegate",
    "cca",
    "Chrome Mobile Apps",
    "Chrome Apps"
  ],
  "dependencies": {
    "archiver": "^1.0.0",
    "cordova-linkto": "0.0.3",
    "cpy": "^4.0.1",
    "exec-then": "^1.3.1",
    "lodash": "^4.13.1",
    "q": "^1.4.1",
    "semver": "^5.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "globby": "^5.0.0",
    "xo": "^0.16.0"
  },
  "scripts": {
    "test": "xo && rm -rf test/tmp && mkdir -p test/tmp && ava --serial ./test/test-gadget.js ./test/test-conf.js ./test/test-platform.js ./test-build.js ./test/test-package.js",
    "test:run": "ava --serial ./test/test-conf.js ./test/test-platform.js ./test/test-run.js"
  },
  "xo": {
    "space": 2,
    "ignores": [
      "test/tmp/**"
    ]
  }
}
