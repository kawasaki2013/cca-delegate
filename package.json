{
  "name": "cca-delegate",
  "version": "0.1.2",
  "description": "Chrome Mobile Apps command delegate for using in programmatically on gulp/Grunt plug-ins",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com"
  },
  "repository": "ragingwind/cca-delegate",
  "license": "MIT",
  "files": [
    "index.js",
    "gadget.js"
  ],
  "keywords": [
    "cca-delegate",
    "cca",
    "Chrome Mobile Apps",
    "Chrome Apps"
  ],
  "dependencies": {
    "archiver": "^0.15.1",
    "cordova-linkto": "0.0.3",
    "cpy": "^3.4.1",
    "exec-then": "^1.3.1",
    "lodash": "^3.10.1",
    "q": "^1.4.1",
    "semver": "^5.0.1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "ava": "^0.9.1",
    "globby": "^3.0.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.2",
    "xo": "^0.12.1"
  },
  "scripts": {
    "test": "xo && ava --serial ./test/test-platform.js",
    "test:config": "mocha ./test/test-conf.js --timeout=400000",
    "test:platform": "mocha ./test/test-platform.js --timeout=400000",
    "test:package": "mocha ./test/test-package.js --timeout=400000"
  },
  "xo": {
    "space": 2,
    "ignores": [
      "test/tmp/**"
    ]
  }
}
